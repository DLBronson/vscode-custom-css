@echo off
setlocal enabledelayedexpansion

REM Prompt the user for the root folder name
set /p root="Enter root folder name: "

REM Create the root folder
mkdir "%root%"
cd "%root%"

REM Create the subfolders
mkdir "folder_a"
mkdir "folder_b"
mkdir "folder_c"

echo Folder structure for %root% created successfully.
pause
